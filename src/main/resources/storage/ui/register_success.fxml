<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity"
            prefHeight="505.0" prefWidth="692.0" styleClass="main-panel"
            stylesheets="@style/register_succes_ui.css" xmlns="http://javafx.com/javafx/8"
            fx:controller="org.parabot.core.ui.controllers.RegisterSuccessController">
    <children>
        <VBox alignment="CENTER" prefHeight="505.0" prefWidth="692.0" AnchorPane.bottomAnchor="0.0"
